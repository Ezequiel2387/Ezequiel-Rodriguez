<div class="container p-5 ">
        <h1>Vista de Productos</h1>
        <table class="table" >
            <thead>
              <tr>
                <th scope="col">Producto</th>
                <th scope="col">Precio</th>
                <th scope="col">Foto</th>
              </tr>
            </thead>
                {{#each data}}
                <tr>
                    <td>{{this.titulo}}</td>
                    <td>{{this.precio}}</td>
                    <td><img  src={{this.url}} hight="70" width="70" /></td>
                </tr>
                {{/each}}
              </tr>
            </tbody>
          </table>
</div>
