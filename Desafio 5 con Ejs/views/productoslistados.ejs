<!DOCTYPE html>
<html lang="en">
    <%- include("templates/head")%>
<body>
    <%- include("templates/nav") %>
    <div class="container p-5 ">
        <h1>Vista de Productos</h1>
        <table class="table" >
            <thead>
              <tr>
                <th scope="col">Producto</th>
                <th scope="col">Precio</th>
                <th scope="col">Foto</th>
              </tr>
            </thead>
            <tbody><% data.forEach(function(pr){%>
              <tr>
                <td><%=pr.titulo%></td>
                <td><%=pr.precio%></td>
                <!-- <td><%=pr.url%></td> -->
                <td><img  src=<%=pr.url %> hight="70" width="70" /></td>
                <%})%>
              </tr>
            </tbody>
          </table>
    </div>
</body>
</html>